function _classCallCheck(instance, Constructor) { if (!(instance instanceof Constructor)) { throw new TypeError("Cannot call a class as a function"); } }



/** 
 * Cross-browser function for dispatching events.
 * @param {EventTarget} target The event target (e.g. an HTML element)
 * @param {string} eventType The event type (e.g. "click")
 * @param {boolean} bubbles Whether the event should bubble (defaults to `false`)
 * @param {boolean} cancelable Whether the event is cancelable (defaults to `false`)
 */

function dispatchEvent(target, eventType) {
  var bubbles = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : false;
  var cancelable = arguments.length > 3 && arguments[3] !== undefined ? arguments[3] : false;

  if (typeof Event === 'function') {
    var event = new Event(eventType, {
      bubbles: bubbles,
      cancelable: cancelable
    });
  } else {
    var event = document.createEvent('Event');
    event.initEvent(eventType, bubbles, cancelable);
  }

  target.dispatchEvent(event);
}
/**
 * Updates the `aria-checked` of the wrapping `label` to match the provided checkbox’ props
 * @param {HTMLElement} checkbox The checkbox element (input[type="checkbox"])
 */


function updateCheckboxAria(checkbox) {
  closest__WEBPACK_IMPORTED_MODULE_0___default()(checkbox, 'label').setAttribute('aria-checked', checkbox.indeterminate ? 'mixed' : checkbox.checked ? 'true' : 'false');
}
/**
 * Sets the `checked` and `indeterminate` props of a checkbox to the correct values for a given status. Also updates the aria-checked attribute of the wrapping `label` element.
 * @param {HTMLElement} checkbox The checkbox element (input[type="checkbox"])
 * @param {mixed} checked Set to `true` for checked, `false` for unchecked, or any other value for indeterminate/mixed state.
 */


function changeCheckboxStatus(checkbox, checked) {
  switch (checked) {
    case true:
      {
        checkbox.indeterminate = false;
        checkbox.checked = true;
      }
      break;

    case false:
      {
        checkbox.indeterminate = false;
        checkbox.checked = false;
      }
      break;

    default:
      {
        checkbox.checked = false;
        checkbox.indeterminate = true;
      }
      break;
  }

  updateCheckboxAria(checkbox);
}
/**
 * Updates the status of a checkbox based on the status of its children.
 * @param {HTMLELement} checkbox The checkbox to update (input[type="checkbox"])
 * @param {HTMLElement} item (Optional) The wrapping item, if already known (for performance)
 */


function updateCheckboxStatus(checkbox, item) {
  if (!item) {
    item = closest__WEBPACK_IMPORTED_MODULE_0___default()(checkbox, '.vgr-checkbox-tree__item');
  }

  var ul = item.querySelector('ul');

  if (!ul) {
    return;
  }

  var checkboxCount = ul.querySelectorAll('input[type="checkbox"]').length;

  if (!checkboxCount) {
    return;
  }

  var checkedCount = ul.querySelectorAll('input[type="checkbox"]:checked').length;

  if (checkedCount === 0) {
    changeCheckboxStatus(checkbox, false);
    return;
  }

  if (checkedCount === checkboxCount && checkboxCount > 1) {
    changeCheckboxStatus(checkbox, true);
    return;
  }

  changeCheckboxStatus(checkbox, null);
}

function propagateCheckboxChange(checkbox) {
  // Update child items
  var item = closest__WEBPACK_IMPORTED_MODULE_0___default()(checkbox, '.vgr-checkbox-tree__item');
  var childCheckboxes = Array.from(item.querySelectorAll('input[type="checkbox"]')).slice(1);
  childCheckboxes.forEach(function (childCheckbox) {
    changeCheckboxStatus(childCheckbox, checkbox.checked);
  }); // Update parent items

  var parents = Object(_utils__WEBPACK_IMPORTED_MODULE_1__["parentsUntil"])(item, '.vgr-checkbox-tree', '.vgr-checkbox-tree__item');
  parents.forEach(function (parent) {
    return updateCheckboxStatus(parent.querySelector('input[type="checkbox"]'), parent);
  });
}

var CheckboxTree = function CheckboxTree(element) {
  var _this = this;

  _classCallCheck(this, CheckboxTree);

  this.changeCheckboxStatusByLabel = function (label, checked) {
    var checkbox = _this.element.querySelector("input[data-label=\"".concat(label, "\"]"));

    if (!checkbox) {
      return;
    }

    changeCheckboxStatus(checkbox, checked);
    propagateCheckboxChange(checkbox);
  };

  this.element = element;
  element.checkboxTree = this;
  Array.from(element.querySelectorAll('.vgr-checkbox-tree__checkbox')).forEach(function (el) {
    el.addEventListener('keydown', function (event) {
      if (event.key === ' ' || event.key === 'Spacebar') {
        event.preventDefault();
        var checkbox = el.querySelector('input[type="checkbox"]');
        changeCheckboxStatus(checkbox, !checkbox.checked);
        dispatchEvent(checkbox, 'change', true);
      }
    });
  });
  Array.from(element.querySelectorAll('input[type="checkbox"]')).forEach(function (checkbox) {
    return updateCheckboxStatus(checkbox);
  });
  element.addEventListener('change', function (event) {
    var checkbox = event.target;

    if (checkbox.nodeName !== 'INPUT' || checkbox.type !== 'checkbox') {
      return;
    }

    updateCheckboxAria(checkbox);
    propagateCheckboxChange(checkbox);
  });
  Array.from(element.querySelectorAll('.vgr-checkbox-tree__toggle')).forEach(function (el) {
    el.addEventListener('click', function (event) {
      event = event || window.event;
      var target = event.currentTarget || event.target;
      var item = closest__WEBPACK_IMPORTED_MODULE_0___default()(target, '.vgr-checkbox-tree__item');

      if (item) {
        var newState = item.getAttribute('aria-expanded') === 'true' ? 'false' : 'true';
        item.setAttribute('aria-expanded', newState);
      }
    });
  });
};



