$.widget('customer.languagepickerToggle', {
    options: {

    },
    // Constructor
    _create: function () {
        this._setupClickEvents();
    },
    _destroy: function () {
    },
    _setupClickEvents: function () {

        var $toolNav = $('.language-block__parent-li [data-tab]');
        var $blockdiv = $('.language-block');

        $toolNav.click(function () {
            if ($blockdiv.attr('data-active') === 'active') {
                $blockdiv.removeAttr('data-active');
            }
            else {
                $blockdiv.attr('data-active', 'active');

            }

        });
    }
});
///#source 1 1 /Static/js/widgets/validation-xform.js

//Getting out the name of integer, positiveinteger, isodate xform type 
var integerName = "";
var positiveintegerName = "";
var isodateName = "";
var array = $("#validate-form").find(".vgr-form__input");
var $errorId = "";
var checkboxItems = $("#validate-form").find('input[type=checkbox]');
var $errorMessages = document.getElementsByClassName("vgr-message--error");
var $errors = $($errorMessages).find(".vgr-message__description");
var submitted = false;
var errorListObject = [];

//Getting out our 'data-v-numeric' for types that we will add to jquery extension
for (var index = 0; index < array.length; ++index) {
    var htmlElement = array[index];
    var attr = $(htmlElement).attr('data-v-numeric');
    if (attr === "integer") {
        integerName = $(htmlElement).attr('data-name');

    }
    if (attr === "positiveinteger") {
        positiveintegerName = $(htmlElement).attr('data-name');
    }
    if (attr === "isodate") {
        isodateName = $(htmlElement).attr('data-name');
    }
}


$('#validate-form').each(function () {

    $(this).validate({

        //Initialize classes an items
        errorClass: "vgr-form__item--error",
        validClass: "vgr-form__item--success",
        element: $('#validate-form').find("vgr-form__item"),



        //Red => Fail element
        highlight: function (element, errorClass, validClass) {
            arrayError = [];
            $errorId = $(element).attr("data-name") + '-error';
            if (element.type === "radio" || element.type === "checkbox") {
                $(element).closest(".vgr-form__item").addClass(errorClass).removeClass(validClass);
            } else {
                $(element).closest(".vgr-form__item").addClass(errorClass).removeClass(validClass);

            }

        },

        //Green => success element
        unhighlight: function (element, errorClass, validClass) {
            if (element.type === "radio" || element.type === "checkbox") {
                $(element).closest(".vgr-form__item").removeClass(errorClass).addClass(validClass);
            } else {
                $(element).closest(".vgr-form__item").removeClass(errorClass).addClass(validClass);
            }

            //if the for doesnt contain errors, hide the errorMessages div
            if (this.numberOfInvalids() === 0) {
                $($errorMessages).addClass("hidden");
            }

        },

        errorPlacement: function (error, element) {
            //if checkbox/readio ska error placera sig under item
            if ($(element).attr("type") === "checkbox" || $(element).attr("type") === "radio") {
                error.insertAfter($(element).closest(".vgr-form__item"));

            } else {
                //for vanlig input ska det givitvis placera under elementen
                error.insertAfter($(element));
            }

        },

        success: function (label, element) {
            //remove the error label from under the validated element 
            label.remove();

            //Loop through erroList object
            var elementToRemove;
            for (var key in errorListObject) {
                if (errorListObject[key]["id"] === element.id) {
                    elementToRemove = errorListObject[key];
                }
            }

            //blocka bort the validated error (successed) from the errorListObject and override the dive content with out new updated errors 
            if (elementToRemove != null) {
                errorListObject.splice($.inArray(elementToRemove, errorListObject), 1);
                var errorContainer = "";
                errorListObject.forEach(function (errorListObject) {
                    errorContainer = errorContainer + errorListObject["message"];
                });
                $($errors).html(errorContainer);
            }


        },

        showErrors: function (errorMap, errorList) {

            if (errorList == null || errorList.length < 1) { //errorList never contains more than one error before submitting.
                this.defaultShowErrors();
                return;
            }

            //Display error-summary
            $($errorMessages).removeClass("hidden");

            //loop through the list whether it's just one error or an error list
            for (var i = 0; i < errorList.length; i++) {
                //Taken the chosen error, adding it in a json object
                var errorListElement = errorList[i];
                var newError = {
                    "id": errorListElement.element.id,
                    "message": '*<label id="' + errorListElement.element.id + '-error' + '">' + $(errorListElement.element.labels).text() + ": " + errorListElement.message + '.</label>' + '<br />'
                };

                //Looking if the old error has the same id change the message to the current error for the element
                var newList = [];
                errorListObject.forEach(function (oldError) {
                    if (oldError["id"] !== errorList[i].element.id) {
                        newList.push(oldError);
                    }
                });
                //puch the error to the error container 
                newList.push(newError);
                errorListObject = newList;
                var errorContainer = "";
                errorListObject.forEach(function (errorListObject) {
                    errorContainer = errorContainer + errorListObject["message"];
                });

                //write out the errors in the dive
                $($errors).html(errorContainer);
            }
            this.defaultShowErrors();
            submitted = false;
        },

        invalidHandler: function (formElement, validator) {
            submitted = true;
        }


    });

});


//Override with xform error messages, can use the others with other form
jQuery.extend(jQuery.validator.messages, {
    test: $("#validate-form").find(".vgr-form__input").attr("data-v-numeric"),
    required: $('#validate-form').find(".vgr-form__input").attr("data-requir"),
    remote: "Please fix this field.",
    email: $('#validate-form').find(".vgr-form__input").attr("data-emailerror"),
    url: "Please enter a valid URL.",
    date: $('#validate-form').find(".vgr-form__input").attr("data-dateerror"),
    dateISO: "Please enter a valid date (ISO).",
    number: "Please enter a valid number.",
    digits: "Please enter only digits.",
    creditcard: "Please enter a valid credit card number.",
    equalTo: "Please enter the same value again.",
    accept: "Please enter a value with a valid extension.",
    maxlength: jQuery.validator.format("Please enter no more than {0} characters."),
    minlength: jQuery.validator.format("Please enter at least {0} characters."),
    rangelength: jQuery.validator.format("Please enter a value between {0} and {1} characters long."),
    range: jQuery.validator.format("Please enter a value between {0} and {1}."),
    max: jQuery.validator.format("Please enter a value less than or equal to {0}."),
    min: jQuery.validator.format("Please enter a value greater than or equal to {0}.")

});


// Delcaring and giving the special error message for our xform special type (integer, positiveinteger, isodate).
var errorMessages = {
    'validation_integer': $('#validate-form').find(".vgr-form__input").attr("data-integererror"),
    'validation_isodate': $('#validate-form').find(".vgr-form__input").attr("data-dateerror"),
    'validation_positiveinteger': $('#validate-form').find(".vgr-form__input").attr("data-positivinteger")
};

//Adding the method for the integer validation
jQuery.validator.addMethod('integer', function (value, element) {
    return this.optional(element) || !isNaN(parseInt(value));
}, errorMessages['validation_integer']);

//Adding the method for the isodate validation
jQuery.validator.addMethod('isodate', function (value, element) {
    return this.optional(element) || value.match(/^[0-9]{4}\-(0[1-9]|1[012])\-(0[1-9]|[12][0-9]|3[01])/);
}, errorMessages['validation_isodate']);

//Adding the method for the positiveinteger validation
jQuery.validator.addMethod('positiveinteger', function (value) {
    return Number(value) > 0;
}, errorMessages['validation_positiveinteger']);



//if the form is not undefined, add to the rules
var formElement = $('#validate-form');
// Check if element exists
if (formElement.length > 0) {
    //Setting the rules for our Xform 
    var setting = formElement.validate().settings;
    setting.rules[integerName] = {
        integer: true
    }
    setting.rules[positiveintegerName] = {
        positiveinteger: true
    }
    setting.rules[isodateName] = {
        isodate: true
    }
}




$("#RemoveEmptyFieldsPrint").click(function () {

    var alista = [];




    var originalBody = document.body;
    
    var container = $(this).closest('.xform-block')[0];
    var origParent = container.parentNode;

    document.body = document.createElement('body');    
    document.body.appendChild(container);

    $('.vgr-form table tr').each(function () {

        var list = [];
        var count = 0;
        $(this).find('.vgr-form__item input, select, textarea').each(function () {
            count++;

            if ($(this).val().trim() == '') {
                list.push(this);
            }

        });

        if (list.length == count) {
            var $parentDiv = $(this).closest('tr');
            $parentDiv.addClass('hideElement');


        }
        else {
            if (list.length > 0) {
                list.forEach(function (element) {
                    var a = this;

                    $(element).addClass('hideElement');
                });
            }
        }
    });

    $('input[type="submit"]').addClass('hideElement');

    window.focus();
    window.print();

    // put the chart back in
    origParent.appendChild(container);

    var hideEl = $(originalBody).find('.hideElement');

    hideEl.each(function () {

        $(this).removeClass('hideElement');

    });

    // restore all body content
    document.body = originalBody;

});







///#source 1 1 /Static/js/typescript/vgr.util.js
var Vgr;
(function (Vgr) {
    var Util;
    (function (Util) {
        $.fn.closest_descendent = function (filter) {
            var $found = $(), $currentSet = this; // Current place
            while ($currentSet.length) {
                $found = $currentSet.filter(filter);
                if ($found.length)
                    break; // At least one match: break loop
                // Get all children of the current set
                $currentSet = $currentSet.children();
            }
            return $found.first(); // Return first match of the collection
        };
        // Generates and returns a unique rfc4122 version 4 compliant GUID
        function getGuid() {
            return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
                var r = Math.random() * 16 | 0, v = c === 'x' ? r : (r & 0x3 | 0x8);
                return v.toString(16);
            });
        }
        Util.getGuid = getGuid;
        // String.prototype.startsWith() with fallback for IE
        function stringStartsWith(sourceString, search, pos) {
            if (!String.prototype.startsWith) {
                return sourceString.substr(!pos || pos < 0 ? 0 : +pos, search.length) === search;
            }
            else {
                return sourceString.startsWith(search, pos);
            }
        }
        Util.stringStartsWith = stringStartsWith;
        // Custom endsWith function because String.prototype.endsWith() doesn't work in IE
        function endsWith(str, suffix) {
            return str.indexOf(suffix, str.length - suffix.length) !== -1;
        }
        Util.endsWith = endsWith;
        function hexToRgb(hex) {
            // Expand shorthand form (e.g. "03F") to full form (e.g. "0033FF")
            var shorthandRegex = /^#?([a-f\d])([a-f\d])([a-f\d])$/i;
            hex = hex.replace(shorthandRegex, function (m, r, g, b) { return (r + r + g + g + b + b); });
            var result = /^#?([a-f\d]{2})([a-f\d]{2})([a-f\d]{2})$/i.exec(hex);
            return result ? {
                r: parseInt(result[1], 16),
                g: parseInt(result[2], 16),
                b: parseInt(result[3], 16)
            } : null;
        }
        Util.hexToRgb = hexToRgb;
        function forceRedrawOfElement(elem) {
            elem.style.display = 'none';
            elem.offsetHeight; // no need to store this anywhere, the reference is enough
            elem.style.display = '';
        }
        Util.forceRedrawOfElement = forceRedrawOfElement;
        /**
         * detect IE
         * returns version of IE or 0, if browser is not Internet Explorer
         */
        function detectIE() {
            var ua = window.navigator.userAgent;
            var msie = ua.indexOf('MSIE ');
            if (msie > 0) {
                // IE 10 or older => return version number
                return parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10);
            }
            var trident = ua.indexOf('Trident/');
            if (trident > 0) {
                // IE 11 => return version number
                var rv = ua.indexOf('rv:');
                return parseInt(ua.substring(rv + 3, ua.indexOf('.', rv)), 10);
            }
            var edge = ua.indexOf('Edge/');
            if (edge > 0) {
                // Edge (IE 12+) => return version number
                return parseInt(ua.substring(edge + 5, ua.indexOf('.', edge)), 10);
            }
            // other browser
            return 0;
        }
        Util.detectIE = detectIE;
        function isFirefox() {
            var isFirefox = navigator.userAgent.toLowerCase().indexOf('firefox') > -1;
            return isFirefox;
        }
        Util.isFirefox = isFirefox;
        // https://stackoverflow.com/a/13348618
        function isChrome() {
            // please note, 
            // that IE11 now returns undefined again for window.chrome
            // and new Opera 30 outputs true for window.chrome
            // but needs to check if window.opr is not undefined
            // and new IE Edge outputs to true now for window.chrome
            // and if not iOS Chrome check
            // so use the below updated condition
            var isChromium = window.chrome;
            var winNav = window.navigator;
            var vendorName = winNav.vendor;
            var isOpera = typeof window.opr !== "undefined";
            var isIEedge = winNav.userAgent.indexOf("Edge") > -1;
            var isIOSChrome = winNav.userAgent.match("CriOS");
            if (isIOSChrome) {
                // is Google Chrome on IOS
                return true;
            }
            else if (isChromium !== null &&
                typeof isChromium !== "undefined" &&
                vendorName === "Google Inc." &&
                isOpera === false &&
                isIEedge === false) {
                // is Google Chrome
                return true;
            }
            else {
                // not Google Chrome 
                return false;
            }
        }
        Util.isChrome = isChrome;
        function getCurrentBreakpoint() {
            // the viewport is at least 1560 pixels wide 
            if (window.matchMedia("(min-width: 1560px)").matches) {
                return 1560;
            }
            // the viewport is less than 1560 pixels wide
            if (window.matchMedia("(min-width: 1024px)").matches) {
                return 1024;
            }
            if (window.matchMedia("(min-width: 768px)").matches) {
                return 768;
            }
            if (window.matchMedia("(min-width: 640px)").matches) {
                return 640;
            }
            if (window.matchMedia("(min-width: 320px)").matches) {
                return 320;
            }
            // Not sure what should be returned as default...
            return window.innerWidth;
        }
        Util.getCurrentBreakpoint = getCurrentBreakpoint;
        function placeCursorAtEndOf(inputElement) {
            // Multiply by 2 to ensure the cursor always ends up at the end;
            // Opera sometimes sees a carriage return as 2 characters.
            var strLength = inputElement.val().length * 2;
            inputElement.focus();
            inputElement[0].setSelectionRange(strLength, strLength);
        }
        Util.placeCursorAtEndOf = placeCursorAtEndOf;
        function queryParamtersAsDictionary() {
            var queryString = window.location.search || '';
            var keyValPairs = [];
            var params = {};
            queryString = queryString.substr(1);
            if (queryString.length) {
                keyValPairs = queryString.split('&');
                for (var i = 0; i < keyValPairs.length; i++) {
                    var key = keyValPairs[i].split('=')[0];
                    if (!key.length)
                        continue;
                    key = key.toLowerCase();
                    if (typeof params[key] === 'undefined')
                        params[key] = [];
                    params[key].push(keyValPairs[i].split('=')[1]);
                }
            }
            return params;
        }
        Util.queryParamtersAsDictionary = queryParamtersAsDictionary;
        function dictonaryToQueryString(dictionary, encode) {
            var str = [];
            for (var p in dictionary) {
                if (encode)
                    str.push(encodeURIComponent(p) + "=" + encodeURIComponent(dictionary[p]));
                else
                    str.push(p + "=" + dictionary[p]);
            }
            return "?" + str.join("&");
        }
        Util.dictonaryToQueryString = dictonaryToQueryString;
        //https://stackoverflow.com/questions/901115/how-can-i-get-query-string-values-in-javascript
        function getParameterByName(name, url) {
            if (!url)
                url = window.location.href;
            name = name.replace(/[\[\]]/g, "\\$&");
            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"), results = regex.exec(url);
            if (!results)
                return null;
            if (!results[2])
                return '';
            return decodeURIComponent(results[2].replace(/\+/g, " "));
        }
        Util.getParameterByName = getParameterByName;
        function trimEnd(myString, char) {
            if (myString.charAt(myString.length - 1) == char) {
                myString = myString.substr(0, myString.length - 1);
            }
            return myString;
        }
        Util.trimEnd = trimEnd;
        // Groups an array by a property
        function groupBy(array, prop) {
            return array.reduce(function (groups, item) {
                var val = item[prop];
                groups[val] = groups[val] || [];
                groups[val].push(item);
                return groups;
            }, {});
        }
        Util.groupBy = groupBy;
        function truncateText(str, length, ending) {
            if (length == null) {
                length = 100;
            }
            if (ending == null) {
                ending = '...';
            }
            if (str.length > length) {
                return str.substring(0, length - ending.length) + ending;
            }
            else {
                return str;
            }
        }
        Util.truncateText = truncateText;
        function getFileExtension(filename) {
            var fileExtension = filename.split('.').pop();
            if (fileExtension)
                fileExtension = fileExtension.toLowerCase();
            return fileExtension;
        }
        Util.getFileExtension = getFileExtension;
        // Checks if the file is a presentation, spreadsheet or document
        function fileIsWorkDocument(filename) {
            var ext = getFileExtension(filename);
            var isWorkDocument = ext == "key" || ext == "odp" || ext == "pps" || ext == "ppt" || ext == "pptx" || // Presentation file formats by file extension
                ext == "ods" || ext == "xlr" || ext == "xls" || ext == "xlsx" || // Spreadsheet file formats by file extension
                ext == "doc" || ext == "docx" || ext == "odt" || ext == "pdf" || ext == "rtf" || ext == "tex" || ext == "txt" || ext == "wks" || ext == "wps" || ext == "wpd"; // Word processor and text file formats by file extension
            return isWorkDocument;
        }
        Util.fileIsWorkDocument = fileIsWorkDocument;
    })(Util = Vgr.Util || (Vgr.Util = {}));
})(Vgr || (Vgr = {}));
//# sourceMappingURL=Vgr.Util.js.map
///#source 1 1 /Static/js/typescript/vgr.ui.js
//interface ChatBotOptions {
//    chatUrl: string;
//    initUrl: string;
//    iconOpen: string;
//    iconClose: string;
//    isOpen: boolean;
//}
//interface ChatBot {
//    mount(node: HTMLElement, opt: ChatBotOptions): void;
//}
//declare var chat: ChatBot
var Vgr;
(function (Vgr) {
    var Ui;
    (function (Ui) {
        var checkboxstatus = {
            allchecked: 'all-checked',
            nonechecked: 'none-checked',
            mixed: 'mixed'
        };
        var MultiCheckbox = /** @class */ (function () {
            function MultiCheckbox(element, resetCheckboxes) {
                var _this = this;
                this.element = element;
                this.resetCheckboxes = resetCheckboxes;
                this.uncheckAllCheckboxes = function () {
                    _this.checkboxGroups.forEach(function (e) {
                        e.subcheckboxes.forEach(function (checkbox) {
                            checkbox.prop('checked', false);
                        });
                    });
                };
                this.setInteractionEvents = function () {
                    _this.checkboxGroups.forEach(function (e) {
                        e.groupcheckbox.click(function () {
                            _this.checkAllSubboxes(e);
                            _this.flipCheckedState(e);
                            _this.countCheckedBoxes();
                        });
                        e.subcheckboxes.forEach(function (checkbox) {
                            checkbox.click(function () {
                                _this.subcheckClick(e);
                            });
                        });
                    });
                };
                this.subcheckClick = function (e) {
                    switch (_this.subCheckboxesStatus(e)) {
                        case checkboxstatus.allchecked:
                            //check the groupbox
                            e.groupcheckbox.prop('checked', true);
                            e.groupIsChecked = true;
                            break;
                        case checkboxstatus.nonechecked:
                            //uncheck the groupbox
                            e.groupcheckbox.prop('checked', false);
                            e.groupIsChecked = false;
                            break;
                        case checkboxstatus.mixed:
                            //uncheck the groupbox
                            e.groupcheckbox.prop('checked', false);
                            e.groupIsChecked = false;
                            break;
                    }
                    _this.countCheckedBoxes();
                };
                this.countCheckedBoxes = function () {
                    _this.numCheckedBoxes = 0;
                    _this.checkboxGroups.forEach(function (e) {
                        var checkedBoxes = e.subcheckboxes.filter(function (f) { return f.prop('checked') === true; });
                        _this.numCheckedBoxes += checkedBoxes.length;
                    });
                    _this.updateButtonLabel();
                };
                this.updateButtonLabel = function () {
                    _this.buttonLabel.html(_this.buttonLabel.data('chosenlabel') + " (" + _this.numCheckedBoxes.toString() + ")");
                };
                this.subCheckboxesStatus = function (e) {
                    if (e.subcheckboxes.every(function (f) { return f.prop('checked') === true; }) && e.subcheckboxes.length > 0)
                        return checkboxstatus.allchecked;
                    if (e.subcheckboxes.every(function (f) { return f.prop('checked') === false; }))
                        return checkboxstatus.nonechecked;
                    return checkboxstatus.mixed;
                };
                this.checkAllSubboxes = function (e) {
                    e.subcheckboxes.forEach(function (checkbox) {
                        checkbox.prop('checked', !e.groupIsChecked);
                    });
                };
                this.flipCheckedState = function (e) {
                    e.groupIsChecked = !e.groupIsChecked;
                };
                this.numCheckedBoxes = 0;
                this.buttonLabel = $('.vgr-multicheckbox__buttonlabel', element);
                this.checkboxGroups = new Array();
                $('[data-group]', element).each(function (i, e) {
                    var subcheckboxes = new Array();
                    $('input[type=checkbox]', e).each(function (i, e) {
                        subcheckboxes.push($(e));
                    });
                    var guid = Vgr.Util.getGuid();
                    var groupElement = $('[data-insertafter]', e);
                    var grouplabelText = groupElement[0].innerHTML;
                    // Optional
                    var grouplabelName = groupElement.data('name');
                    var grouplabelSelected = groupElement.data('selected');
                    var grouplabelValue = groupElement.data('value');
                    var grouplabelChecked = groupElement.data('checked');
                    var projectsearchblock = $('.projectsearchblock');
                    var Checked = grouplabelSelected && grouplabelSelected === "True" ? "data-checked" : "";
                    var grouplabel = $('[data-insertafter]', e).after("<label data-grouplabel id=\"" + guid + "\">" + grouplabelText + "</label>").next();
                    $("[id=" + guid + "]", e).prepend("<input type=\"checkbox\" " + Checked + " name=\"" + grouplabelName + "\" value=\"" + grouplabelValue + "\" />").next();
                    var groupcheckbox = $("[id=" + guid + "]", e).find('input[type=checkbox]');
                    _this.checkboxGroups.push({
                        grouplabel: grouplabel,
                        groupcheckbox: groupcheckbox,
                        subcheckboxes: subcheckboxes,
                        groupIsChecked: false
                    });
                });
                if (resetCheckboxes) {
                    this.uncheckAllCheckboxes();
                }
                else {
                    this.checkboxGroups.forEach(function (group) {
                        // Check if the group should be selected
                        _this.subcheckClick(group);
                    });
                }
                this.setInteractionEvents();
                $('[data-checked]').prop("checked", true);
            }
            return MultiCheckbox;
        }());
        Ui.MultiCheckbox = MultiCheckbox;
        var Toggle = /** @class */ (function () {
            function Toggle(element, tab, panel, focusElement, togglestyle, closeOnOutsideClick) {
                var _this = this;
                this.element = element;
                this.setAttributes = function () {
                    var guid = Vgr.Util.getGuid();
                    // Set a data attribute on the container element, used to indicate status of expansion - for CSS purposes
                    _this.element.attr('data-isexpanded', _this.isExpanded.toString());
                    _this.element.attr('aria-expanded', _this.isExpanded.toString());
                    //this.panel.attr('closeOnOutsideClick', this.isExpanded.toString());
                    // Set the role of the 'tab element' to link
                    if (!$(_this.tab).is(":button")) {
                        _this.tab.attr('role', 'link');
                    }
                    // Set tabindex to 0 in order to tab-focus the 'tab element', may or may not be needed depending on what kind of element it is
                    //this.tab.attr('tabindex', '0');
                    // Set which element the 'tab element' opens, for screen readers
                    _this.tab.attr('aria-controls', guid);
                    // Set the panels id to correspond to our guid
                    _this.panel.attr('id', guid);
                    // Set the panels tabindex to -1, this will make it focusable via code - but not via keyboard tabbing
                    _this.panel.attr('tabindex', '-1');
                    // Set the panels role to tabpanel
                    //this.panel.attr('role', 'tabpanel');
                    // Set the panels aria-expanded state
                    _this.panel.attr('aria-expanded', _this.isExpanded.toString());
                    _this.panel.attr('aria-hidden', (!(_this.isExpanded)).toString());
                };
                this.setScreenReaderText = function () {
                    _this.screenReaderText.element.text(_this.isExpanded
                        ? _this.screenReaderText.hidetext
                        : _this.screenReaderText.showtext);
                };
                this.setInteractionEvents = function () {
                    var self = _this;
                    // var close = this.panel.attr('closeOnOutsideClick').valueOf();
                    //if (this.closeOnOutsideClick) {
                    //    $('body').on('click', function (e) {
                    //        if (e.target.className != 'radiolist' && $(e.target).closest('.radiolist').length == 0 && e.target.className != 'vgr-expand-dropdownListForCitys' && $(e.target).closest('.vgr-expand-dropdownListForCitys').length == 0) {
                    //            $('.radiolist').hide();
                    //        }
                    //        else {
                    //            $('.radiolist').toggle();
                    //            $(document).on('keyup.escape', (e) => {
                    //                if (e.keyCode === 27) {
                    //                    $('.list-heading').trigger('click');
                    //                }
                    //            });
                    //        }
                    //    });
                    //}
                    // Change the label to the checked radio button on page load
                    $('.radiolist ul li input[type="radio"]').each(function () {
                        if ($(this).is(':checked')) {
                            var titleOfSeletedItem = $("label[for='" + this.value + "']").text();
                            $('.list-heading').text(titleOfSeletedItem);
                            return false; // Break the loop as we have already set the text
                        }
                    });
                    _this.tab.click(function () {
                        var self = _this;
                        if (_this.closeOnOutsideClick) {
                            // When a radio button is clicked
                            $('.radiolist ul li input[type="radio"]').on('click', function () {
                                var label = $("label[for='" + this.value + "']");
                                // Set the drop down label to the selected radio button
                                $('.list-heading').text(label.text());
                                // Hide the panel
                                self.toggleFunction();
                                self.flipExpandedState();
                                self.setScreenReaderText();
                                // Unbind click events
                                $('body').unbind('click');
                                $('.radiolist ul li input[type="radio"]').unbind('click');
                                $(document).unbind('keypress');
                            });
                        }
                        _this.toggleFunction();
                        _this.flipExpandedState();
                        _this.setScreenReaderText();
                        if (_this.isExpanded) {
                            // 'Esc' keyup = 27
                            $(document).on('keyup.escape', function (e) {
                                if (e.keyCode === 27) {
                                    _this.tab.trigger('click');
                                }
                            });
                            if (self.closeOnOutsideClick) {
                                // Handle keyboard navigation for radio button list with a checkbox at the top.
                                $(document).bind('keypress', function (e) {
                                    if ((e.keyCode ? e.keyCode : e.which) === 32) {
                                        e.preventDefault();
                                        if (document.activeElement) {
                                            // Check if the focused element is a child of the panel
                                            var isInPanel = $(document.activeElement).parent(".radiolist") != undefined;
                                            if (isInPanel) {
                                                // the checkbox is clicked and not the label
                                                if ($(document.activeElement).is(':checkbox') == true) {
                                                    $(document.activeElement).click();
                                                }
                                                else {
                                                    // Get the clicked label's for attribute
                                                    var labelFor = $(document.activeElement).attr('for');
                                                    var focusedInput = $('#' + $.escapeSelector(labelFor)); // Use escapeSelector() because the id may contain some invalid characthers
                                                    if (focusedInput)
                                                        $(focusedInput).click();
                                                }
                                            }
                                        }
                                    }
                                });
                                // Add click event to the body to check if the user clicks outside the panel
                                $('body').on('click', function (e) {
                                    // Check that the click isn't in the panel or the drop down button
                                    if (e.target.className != 'radiolist' && $(e.target).closest('.radiolist').length == 0 && e.target.className != 'vgr-expand-dropdownListForCitys' && $(e.target).closest('.vgr-expand-dropdownListForCitys').length == 0) {
                                        // Hide the panel
                                        self.toggleFunction();
                                        self.flipExpandedState();
                                        self.setScreenReaderText();
                                        // Unbind click events
                                        $('body').unbind('click');
                                        $('.radiolist ul li input[type="radio"]').unbind('click');
                                        $(document).unbind('keypress');
                                    }
                                });
                            }
                        }
                        else {
                            // Dont forget to unbind all events when the panel isn't active!
                            $(document).unbind('keyup.escape');
                            $('body').unbind('click');
                            $('.radiolist ul li input[type="radio"]').unbind('click');
                            $(document).unbind('keypress');
                        }
                        _this.deleteFocus();
                    });
                    // 'Enter' keypress = 13            
                    _this.tab.keypress(function (e) {
                        if (e.which === 13 || e.which === 32) {
                            // preventDefault to avoid default 'click' event of pressing enter on elements such as 'button', 'a' etc. This would cause double-clicks.
                            e.preventDefault();
                            _this.tab.trigger('click');
                            e.target.className += ' active';
                        }
                    });
                };
                this.deleteFocus = function () {
                    _this.tab.blur();
                    $(':button').removeClass('active');
                };
                this.flipExpandedState = function () {
                    _this.isExpanded = !_this.isExpanded;
                    _this.element.attr('data-isexpanded', _this.isExpanded.toString());
                    _this.element.attr('aria-expanded', _this.isExpanded.toString());
                    //this.panel.attr('closeonoutsideclick', this.isExpanded.toString());
                    _this.panel.attr('aria-expanded', _this.isExpanded.toString());
                    _this.panel.attr('aria-hidden', (!(_this.isExpanded)).toString());
                };
                this.triggerClick = function () {
                    _this.tab.trigger('click');
                };
                // Select the closest data-tab in case toggle buttons are nested.
                this.tab = tab ? tab : $(this.element).closest_descendent("[data-tab]:first");
                this.panel = panel ? panel : $('[data-panel]:first', this.element);
                this.focusElement = focusElement ? focusElement : this.panel;
                var toggleStyle = togglestyle ? togglestyle : element.data('togglestyle');
                this.closeOnOutsideClick = closeOnOutsideClick;
                // Type of toggle style
                switch (toggleStyle) {
                    case 'fade':
                        this.toggleFunction = function () {
                            _this.panel.fadeToggle('fast');
                        };
                        break;
                    case 'slide':
                        this.toggleFunction = function () {
                            _this.panel.slideToggle();
                        };
                        break;
                    case 'class':
                        this.toggleFunction = function () {
                            _this.panel.toggleClass('open');
                        };
                        break;
                    case 'mobilemenu':
                        this.toggleFunction = function () {
                            if (_this.isExpanded) {
                                // Close the menu
                                _this.panel.transition({ x: '110%' }, 200, function () {
                                    _this.panel.toggleClass('open');
                                });
                                var eventClose = document.createEvent('Event');
                                eventClose.initEvent('mobilemenu-close', true, true);
                                document.dispatchEvent(eventClose);
                            }
                            else {
                                // Open the menu
                                _this.panel.toggleClass('open');
                                _this.panel.transition({ x: '0' }, 200);
                                var eventOpen = document.createEvent('Event');
                                eventOpen.initEvent('mobilemenu-open', true, true);
                                document.dispatchEvent(eventOpen);
                            }
                        };
                        break;
                    default:
                        this.toggleFunction = function () {
                            _this.panel.toggle();
                        };
                }
                this.isExpanded = element.is('[data-startopen]');
                var $sr = $('span[data-showtext]', this.tab);
                this.screenReaderText = {
                    element: $sr,
                    showtext: $sr.data('showtext'),
                    hidetext: $sr.data('hidetext')
                };
                this.setAttributes();
                this.setInteractionEvents();
                this.setScreenReaderText();
            }
            return Toggle;
        }());
        Ui.Toggle = Toggle;
        var MobileMenu = /** @class */ (function () {
            function MobileMenu(menu, menubtn, closebtn) {
                var _this = this;
                this.menu = menu;
                this.menubtn = menubtn;
                this.closebtn = closebtn;
                this.menuItems = [];
                this.toolMenuItems = [];
                this.setupEvents = function () {
                    $(document).on('mobilemenu-open', function () {
                        _this.overlay.fadeIn();
                        _this.body.toggleClass('lock-body');
                        _this.html.toggleClass('lock-body');
                        if (_this.activeMenuItem !== undefined)
                            _this.activeMenuItem.menuitem[0].scrollIntoView();
                    });
                    $(document).on('mobilemenu-close', function () {
                        _this.overlay.fadeOut();
                        _this.body.toggleClass('lock-body');
                        _this.html.toggleClass('lock-body');
                    });
                    _this.closebtn.click(function () {
                        _this.menubtntoggle.triggerClick();
                    });
                    var map = [];
                    onkeydown = onkeyup = function (e) {
                        e = e || event; // to deal with IE
                        map[e.keyCode] = e.type === 'keydown';
                    };
                    $(document).on('keyup', function (e) {
                        var activeElement = $(document.activeElement);
                        // lock-body == menu is open
                        if (_this.body.hasClass('lock-body')) {
                            var index = activeElement.parent().parent().data('index');
                            // ARROW UP
                            if (map[38]) {
                                // Initial focus is on the whole menu OR focus on the close button
                                if (activeElement.hasClass('vgr-mobilemenu') || activeElement[0] === _this.closebtn[0]) {
                                    _this.toolMenuItems[_this.toolMenuItems.length - 1].link.focus();
                                }
                                else if (activeElement.hasClass('tool-navigation__link')) {
                                    index = activeElement.data('index');
                                    if (index === 0) {
                                        _this.focusPreviousItem(_this.menuItems[_this.menuItems.length - 1].menuitem.data('index') + 1);
                                    }
                                    else {
                                        _this.toolMenuItems[index - 1].link.focus();
                                    }
                                }
                                else if (activeElement.hasClass('vgr-mobilemenu__link')) {
                                    _this.focusPreviousItem(index);
                                }
                                else if (activeElement.hasClass('vgr-mobilemenu__list')) {
                                    activeElement.prev().find('a').first().focus();
                                }
                            }
                            // ARROW DOWN
                            if (map[40]) {
                                // Initial focus is on the whole menu
                                if (activeElement.hasClass('vgr-mobilemenu')) {
                                    _this.closebtn.focus();
                                }
                                else if (activeElement[0] === _this.closebtn[0]) {
                                    _this.menuItems[0].link.focus();
                                }
                                else if (activeElement.hasClass('vgr-mobilemenu__link')) {
                                    _this.focusNextItem(index);
                                }
                                else if (activeElement.hasClass('tool-navigation__link')) {
                                    index = activeElement.data('index');
                                    if (index + 1 === _this.toolMenuItems.length) {
                                        _this.closebtn.focus();
                                    }
                                    else {
                                        _this.toolMenuItems[index + 1].link.focus();
                                    }
                                }
                                else if (activeElement.hasClass('vgr-mobilemenu__list')) {
                                    activeElement.find('a').first().focus();
                                }
                            }
                            // ARROW RIGHT 
                            if (map[39]) {
                                if (activeElement.hasClass('vgr-mobilemenu__link')) {
                                    if (_this.menuItems[index].hasSubMenu) {
                                        activeElement.next().focus();
                                    }
                                }
                                else if (activeElement.hasClass('tool-navigation__link')) {
                                    index = activeElement.data('index');
                                    if (index + 1 === _this.toolMenuItems.length) {
                                        _this.closebtn.focus();
                                    }
                                    else {
                                        _this.toolMenuItems[index + 1].link.focus();
                                    }
                                }
                            }
                            // ARROW LEFT
                            if (map[37]) {
                                if (activeElement.hasClass('vgr-mobilemenu__leveltoggle')) {
                                    activeElement.prev().focus();
                                }
                                else if (activeElement.hasClass('vgr-mobilemenu__list')) {
                                    activeElement.prev().find('a').first().focus();
                                }
                                else if (activeElement.hasClass('tool-navigation__link')) {
                                    index = activeElement.data('index');
                                    if (index === 0) {
                                        _this.focusPreviousItem(_this.menuItems[_this.menuItems.length - 1].menuitem.data('index') + 1);
                                    }
                                    else {
                                        _this.toolMenuItems[index - 1].link.focus();
                                    }
                                }
                            }
                            //TAB
                            if (map[9]) {
                                if (_this.previousActiveElement == $('.vgr-mobilemenu__footer .main-navigation__ul li:last-child .tool-navigation__link')[0]) {
                                    _this.closebtn.focus();
                                }
                                _this.previousActiveElement = activeElement[0];
                            }
                        }
                    });
                };
                this.focusPreviousItem = function (index) {
                    if (index === 0) {
                        return _this.closebtn.focus();
                    }
                    if (_this.menuItems[index - 1].link.is(':visible')) {
                        return _this.menuItems[index - 1].link.focus();
                    }
                    _this.focusPreviousItem(index - 1);
                };
                this.focusNextItem = function (index) {
                    if (index + 1 === _this.menuItems.length) {
                        // Try to find the first tool menu item
                        var footeritem = _this.menu.find('.vgr-mobilemenu__footer a').first();
                        return footeritem.length === 1 ? footeritem.focus() : _this.closebtn.focus();
                    }
                    if (_this.menuItems[index + 1].link.is(':visible')) {
                        return _this.menuItems[index + 1].link.focus();
                    }
                    _this.focusNextItem(index + 1);
                };
                this.addMenuItems = function (menu) {
                    menu.find('.vgr-mobilemenu__item').each(function (i, e) {
                        var menuitem = new MenuItem(_this, $(e), _this.screenReaderText, i);
                        _this.menuItems.push(menuitem);
                        if (menuitem.menuitem.hasClass('vgr-mobilemenu__item--active')) {
                            _this.activeMenuItem = menuitem;
                        }
                    });
                };
                this.screenReaderText = menu.data('srtext');
                this.overlay = $('.darkened-overlay');
                this.body = $('body');
                this.html = $('html');
                this.addMenuItems(menu);
                menu.find('.tool-navigation__li').each(function (i, e) {
                    _this.toolMenuItems.push(new ToolMenuItem($(e), i));
                });
                this.menubtntoggle = new Toggle(menu, menubtn, menu, null, 'mobilemenu');
                this.closebtn.attr('role', 'tab');
                this.closebtn.attr('tabindex', '0');
                this.closebtn.attr('aria-controls', this.menubtntoggle.guid);
                this.setupEvents();
            }
            return MobileMenu;
        }());
        Ui.MobileMenu = MobileMenu;
        var ToolMenuItem = /** @class */ (function () {
            function ToolMenuItem(toolmenuitem, index) {
                this.toolmenuitem = toolmenuitem;
                this.index = index;
                this.link = toolmenuitem.find('.tool-navigation__link');
                this.link.attr('data-index', index);
            }
            return ToolMenuItem;
        }());
        var MenuItem = /** @class */ (function () {
            function MenuItem(mobileMenu, menuitem, srtext, index) {
                var _this = this;
                this.mobileMenu = mobileMenu;
                this.menuitem = menuitem;
                this.srtext = srtext;
                this.index = index;
                this.setupClickEvent = function () {
                    _this.openBtn.click(function () {
                        // The sub-menu should already be fetched and opened if the current page is a subpage, so then we don't need to do it again
                        var alreadyOpened = _this.openBtn.is('[data-startopen]');
                        if (!_this.hasPopulatedSubMenu && !alreadyOpened) {
                            var pageId = _this.openBtn.attr('data-pageid');
                            var self = _this;
                            // Get more menu items and append them to the html
                            $.ajax({
                                url: '/GetMoreMenuPages',
                                type: 'GET',
                                data: {
                                    pageId: pageId
                                },
                                dataType: 'json',
                                success: function (result) {
                                    if (result.error) {
                                    }
                                    else {
                                        self.openBtn.parents('li') // Find the menu item that button exists in
                                            .first() // Make sure we only select the closest li
                                            .append(result.html); // Append the sub-menu structure to the list item     
                                        // Let us know that the open menu button has already been pressed and that we should not fetch sub-menu item for this node again
                                        self.hasPopulatedSubMenu = true;
                                        var submenu = self.menuitem.find('ul').first();
                                        if (submenu.length > 0) {
                                            self.hasSubMenu = true;
                                            self.subMenu = submenu;
                                            self.openBtn = self.menuitem.find('button').first();
                                            self.setupToggleEvent();
                                            // Add recently fetch items to the menu
                                            self.mobileMenu.addMenuItems(self.subMenu);
                                            // Because the toggle above only sets up the event it won't actually expand unless we call the click event
                                            self.openBtn.click();
                                            self.openBtn.addClass("active");
                                        }
                                    }
                                }
                            });
                        }
                    });
                };
                this.setupToggleEvent = function () {
                    _this.subMenu.attr('aria-label', _this.srtext + _this.link.html());
                    var toggle = new Toggle(_this.openBtn, _this.openBtn, _this.subMenu, null, 'slide');
                };
                this.link = menuitem.find('a').first();
                var openBtn = menuitem.find('button').first();
                // If this button exists on the list item then the page has children
                if (openBtn.length > 0) {
                    this.hasSubMenu = true;
                    this.openBtn = openBtn;
                    this.setupClickEvent();
                }
                // A sub-menu can exist on page-load if the current page is somewhere down the page tree.
                var submenu = menuitem.find('ul').first();
                if (submenu.length > 0) {
                    this.subMenu = submenu;
                    this.setupToggleEvent();
                }
            }
            return MenuItem;
        }());
        //export class ChatBot {
