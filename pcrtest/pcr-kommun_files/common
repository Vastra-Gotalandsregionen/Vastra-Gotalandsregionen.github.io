(function ($) {
    $.fn.goTo = function () {
        $('html, body').animate({
            scrollTop: $(this).offset().top + 'px'
        }, 'fast');
        return this; // for chaining...
    }
})(jQuery);

var dropdownSelector = '.notice-of-intrest #EmailReciver';
$(dropdownSelector).select2({ width: '100%' });

var input = $('#notice-of-interest-form .input-validation-error:first');

if (input.length > 0) {

    $(input).goTo();
    // If this is the dropdown box. (Focus does not automatically work on the dropdown)
    if (input.attr('id') === 'EmailReciver') {
        // Activate focus manually
        $(dropdownSelector).select2('open').select2('close');
    } else {
        input.focus(); // Focus on first invalid field
    }
}
