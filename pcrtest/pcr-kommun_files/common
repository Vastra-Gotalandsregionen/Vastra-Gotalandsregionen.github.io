function parentsUntil(element, until, selector) {
  var parent = element.parentNode;
  var parents = [];

  while (parent && parent !== document) {
    if (selector == null || matches_selector__WEBPACK_IMPORTED_MODULE_0___default()(parent, selector)) {
      parents.push(parent);
    }

    if (matches_selector__WEBPACK_IMPORTED_MODULE_0___default()(parent, until)) break;
    parent = parent.parentNode;
  }

  return parents;
}

