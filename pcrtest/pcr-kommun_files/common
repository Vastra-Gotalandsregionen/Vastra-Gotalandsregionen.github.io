///#source 1 1 /Static/js/widgets/cascading-dropdownlist.js
$.widget('customer.cascadingDropdownlist', {
    options: {

    },

    // Constructor
    _create: function () {
        this._setupDropdownlist();
    },
    _destroy: function () { },
    _setupDropdownlist: function () {
        var self = this;
        var $dropdowns = $(this.element);
        $.each($dropdowns, function () {
            var $dropdown = $(this);
            var $targetDropdown = $($dropdown.attr("data-target"));
            var targetListUrl = $dropdown.attr("data-url");
            var targetSelectedValue = $dropdown.attr("data-target-selected-value");
            var targetDefaultOptionText = $dropdown.attr("data-target-default-option-text");

            if ($(this).val() > 0) {
                self._bindData($dropdown.val(), $targetDropdown, targetListUrl, targetSelectedValue, targetDefaultOptionText);
            }

            $dropdown.click(function () {
                self._bindData($(this).val(), $targetDropdown, targetListUrl, targetSelectedValue, targetDefaultOptionText);
            });
        });
    },
    _bindData: function (id, $targetDropdown, targetListUrl, targetSelectedValue, targetDefaultOptionText) {
        $.ajax({
            url: targetListUrl,
            data: { id: id },
            cache: false,
            type: "GET",
            success: function (data) {
                var markup = "<option value='0'>" + targetDefaultOptionText + "</option>";

                $.each(data, function () {
                    if (this.Value === targetSelectedValue) {
                        markup += "<option value=" + this.Value + " selected>" + this.Text + "</option>";
                    } else {
                        markup += "<option value=" + this.Value + ">" + this.Text + "</option>";
                    }

                });

                $targetDropdown.html(markup).show();
            },
            error: function (reponse) {
                alert("error : " + reponse);
            }
        });
    }
});
///#source 1 1 /Areas/Public/Static/js/accessibility-aid-block.js
$(document).ready(function () {

});

$("[data-toggle-button]").each(function () {
    var toggleButtonTarget = $(this).attr("data-toggle-target");
    $(toggleButtonTarget).toggle(false);

    $(this).on("click", function () {
        $(toggleButtonTarget).toggle();
    });
});



///#source 1 1 /Areas/Public/Static/js/AutocompleteSearch.js
//*********Extended Autocomplete*********//
function initializeAutocomplete() {

    var useAutocomplete = $(document.body).data("useautocomplete") == true;

    if (!useAutocomplete)
        return;

    var urlRightPart = location.pathname;

    // Append a slash if it's missing
    if (!endsWith(urlRightPart, "/")) {
        urlRightPart += "/";
    }

    var urlPath = urlRightPart + "HittaQueryCompletion";

    var searchField = $("#search");
    var resultContainer = $("#autocomplete-result");
    var searchButton = $("#search-button");
    var topDiv = $("#result-topdiv");
    var bottomDiv = $("#result-bottomdiv");
    resultContainer.append(topDiv);
    resultContainer.append(bottomDiv);
    addCloseResultContainerListener(searchField, searchButton, resultContainer);

    searchField.autocomplete({
        minLength: 1,
        delay: 400,
        source: function (request) {
            $.ajax({
                dataType: "json",
                url: urlPath,
                type: "POST",
                data: { q: request.term, scope: getScopeUrlParam() },
                success: function (data) {
                    if (data === "") return;
                    var requestFrom = data.substring(0, 1); //i/e, internal or external
                    var jsObject = JSON.parse(data.substring(1));
                    if (jsObject.suggestions.length === 0) {
                        resultContainer.addClass("hidden");
                        return;
                    }
                    topDiv.html("");
                    bottomDiv.html("");

                    resultContainer.removeClass("hidden");

                    var searchQueries = $(jsObject.suggestions).filter(function (i, n) { return n.category === "search_query"; });
                    var www = $(jsObject.suggestions).filter(function (i, n) { return n.category === "www"; });
                    var dokument = $(jsObject.suggestions).filter(function (i, n) { return n.category === "dokument"; });
                    var verksamhet = $(jsObject.suggestions).filter(function (i, n) { return n.category === "verksamhet"; });
                    var person = $(jsObject.suggestions).filter(function (i, n) { return n.category === "person"; });

                    addSearchQueries(searchQueries, topDiv);
                    addWww(www, topDiv);
                    addDokument(dokument, bottomDiv);
                    if (requestFrom === "e") {
                        addVerksamhet(verksamhet, bottomDiv); //add this when verksamhet is activated in the system
                    } else {
                        addPerson(person, bottomDiv);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {

                }
            });
        }
    });
}

// Custom endsWith function because String.prototype.endsWith() doesn't work in IE
function endsWith(str, suffix) {
    return str.indexOf(suffix, str.length - suffix.length) !== -1;
}

function getUrlParameter(sParam) {
    var sPageURL = decodeURIComponent(window.location.search.substring(1)),
        sURLVariables = sPageURL.split('&'),
        sParameterName,
        i;

    for (i = 0; i < sURLVariables.length; i++) {
        sParameterName = sURLVariables[i].split('=');

        if (sParameterName[0].toLowerCase() === sParam.toLowerCase()) {
            return sParameterName[1] === undefined ? true : sParameterName[1];
        }
    }
};

function getScopeUrlParam() {
    var scope = getUrlParameter("scope");
    if (!scope)
        scope = getUrlParameter("s");
    return scope;
}

function addSearchQueries(queries, container) {
    var form = $("#search-form");
    var url = form.attr("action") + "?qc=true&q=";


    var scope = getScopeUrlParam();
    var scopeQuery = "";
    if (scope)
        scopeQuery = "&scope=" + scope;

    var ulList = $(container).append("<ul id='searchquery-list' ></ul>").find("#searchquery-list");
    var header = $("<LH>", { html: "<h2>S&ouml;kordsf&ouml;rslag<h2>" }).appendTo(ulList);
    header.addClass("autocomplete-resultheader");
    if (queries.length < 1) return;
    $.each(queries, function (i) {
        var li = $("<li/>").appendTo(ulList);
        var h3 = $("<h3>").appendTo(li);
        var aaa = $("<a>", {
            html: queries[i].label_hl,
            href: url + queries[i].label + scopeQuery
        }).appendTo(h3);
    });
}

function addWww(queries, container) {
    var ulList = $(container).append("<ul id='www-list' ></ul>").find("#www-list");
    var header = $("<LH>", { html: "<h2>Webb</h2>" }).appendTo(ulList);
    header.addClass("autocomplete-resultheader");
    if (queries.length < 1) return;
    $.each(queries, function (i) {
        var li = $("<li/>").appendTo(ulList);
        li.addClass("image web");
        var h3 = $("<h3>").appendTo(li);
        var aaa = $("<a>", {
            html: queries[i].label,
            href: queries[i].url
        }).appendTo(h3);
        if (queries[i].revisiondate != null || queries[i].scopelabel != null) {
            var source = $("<p>", {
                html: (queries[i].revisiondate != null ? queries[i].revisiondate : "") + (queries[i].scopelabel != null ? " - " + queries[i].scopelabel : "")
            }).appendTo(li);
        }
    });
}

function addDokument(queries, container) {
    var ulList = $(container).append("<ul id='dokument-list' ></ul>").find("#dokument-list");
    var header = $("<LH>", { html: "<h2>Dokument</h2>" }).appendTo(ulList);
    header.addClass("autocomplete-resultheader");
    if (queries.length < 1) return;
    $.each(queries, function (i) {
        var format = (queries[i].format != null ? "file " + queries[i].format.substring(queries[i].format.lastIndexOf(".") + 1) : "file");
        var li = $("<li/>").appendTo(ulList);
        li.addClass(format);
        var h3 = $("<h3>").appendTo(li);
        var aaa = $("<a>", {
            html: queries[i].label,
            href: queries[i].url
        }).appendTo(h3);
        if (queries[i].revisiondate != null || queries[i].scopelabel != null) {
            var source = $("<p>", {
                html: (queries[i].revisiondate != null ? queries[i].revisiondate : "") + (queries[i].scopelabel != null ? " - " + queries[i].scopelabel : "")
            }).appendTo(li);
        }
    });
}

function addVerksamhet(queries, container) {
    var ulList = $(container).append("<ul id='verksamhet-list' ></ul>").find("#verksamhet-list");
    var header = $("<LH>", { html: "<h2>Verksamhet</h2>" }).appendTo(ulList);
    header.addClass("autocomplete-resultheader");
    if (queries.length < 1) return;
    $.each(queries, function (i) {
        var li = $("<li/>").appendTo(ulList);
        li.addClass("image business");

        var h3 = $("<h3>").appendTo(li);

        var href = '';
        if (queries[i].hsaIdentity !== undefined) {
            href = '/sok-verksamheter?hsaIdentity=' + queries[i].hsaIdentity;
        } else {
            href = queries[i].url;
        }

        var aaa = $("<a>", {
            html: queries[i].label,
            href: href
        }).appendTo(h3);

    if (queries[i].phonenumber != null) {
        var source = $("<p>", {
            html: "<a href='tel:" + queries[i].phonenumber + "' target='_top'>" + queries[i].phonenumber + "</a>"
        }).appendTo(li);
    }

});
}

function addPerson(queries, container) {
    var ulList = $(container).append("<ul id='person-list' ></ul>").find("#person-list");
    var header = $("<LH>", { html: "<h2>Person</h2>" }).appendTo(ulList);
    header.addClass("autocomplete-resultheader");
    if (queries.length < 1) return;
    $.each(queries, function (i) {
        var li = $("<li/>").appendTo(ulList);
        li.addClass("image person");
        var h3 = $("<h3>").appendTo(li);

        var href = '';
        if (queries[i].vgrid !== undefined) {
            href = '/sok-person?vgrid=' + queries[i].vgrid;
        } else {
            href = queries[i].url;
        }

        var aaa = $("<a>", {
            html: queries[i].label,
            href: href
        }).appendTo(h3);
        if (queries[i].email != null) {
            var source = $("<p>", {
                html: "<a href='mailto:" + queries[i].email + "' target='_top'>" + queries[i].email + "</a>"
            }).appendTo(li);
        }
    });
}

function addCloseResultContainerListener(searchField, searchButton, resultContainer) {
    closeResultContainerMousedown(resultContainer);
    closeResultContainerFocusin(searchField, searchButton, resultContainer);
    closeResultContainerEsc(resultContainer);
}
function closeResultContainerMousedown(resultContainer) {
    $("body").mousedown(function (e) {
        if (resultContainer.length > 0) {
            if (e.target.offsetParent == null || e.target.offsetParent.id !== resultContainer[0].id) {
                resultContainer.addClass("hidden");
            }
        }
    });
}
function closeResultContainerFocusin(searchField, searchButton, resultContainer) {
    $("body").focusin(function (e) {
        if (resultContainer.length > 0) {
            var focused = document.activeElement.id;
            if (focused !== searchField[0].id && focused !== searchButton[0].id && e.target.offsetParent && e.target.offsetParent.id !== resultContainer[0].id) {
                resultContainer.addClass("hidden");
            }
        }
    });
}
function closeResultContainerEsc(resultContainer) {
    $(document).on("keyup", function (evt) {
        if (evt.keyCode == 27) {
            resultContainer.addClass("hidden");
        }
    });
}
///#source 1 1 /Static/js/widgets/bestallarid-querycompletion.js
$.widget('customer.bestallarIdQueryCompletion', {
    options: {

    },

    // Constructor
    _create: function () {
        this._initQueryCompletion();
        this._handleClickEvent();
    },
    _destroy: function () { },
    _initQueryCompletion: function () {
        var searchField = $("#BestalladIdSearchQuery");
        var searchButton = $("#BestalladIdSearchButton");
        var resultContainer = $(".autocomplete-result");

        Vgr.Util.placeCursorAtEndOf(searchField);

        var urlRightPart = location.pathname;
        if (!Vgr.Util.endsWith(urlRightPart, "/")) {
            urlRightPart += "/";
        }

        var urlPath = urlRightPart + "BestallarIdQueryCompletion";

        this._addCloseResultContainerListener(searchField, searchButton, resultContainer);

        var self = this;
        searchField.autocomplete({
            minLength: 1,
            delay: 300,
            source: function (request) {
                $.ajax({
                    dataType: "json",
                    url: urlPath,
                    type: "POST",
                    data: { q: searchField.val() },
                    success: function (data) {
                        if (data === "") return;
                        var jsObject = JSON.parse(data);
                        if (jsObject.suggestions.length === 0) {
                            resultContainer.addClass("hidden");
                            return;
                        }

                        // Show the suggestions
                        resultContainer.html("");
                        resultContainer.removeClass("hidden");

                        self._addSearchQueries(jsObject.suggestions, resultContainer, searchField);
                    }
                });
            },
            select: function (suggestion) {
                console.log('You selected: ' + suggestion.value + ', ' + suggestion.data);
            },
            focus: function (event, ui) {
                console.log('focused: ' + ui.item.label);
                // or $('#autocomplete-input').val(ui.item.label);

                // Prevent the default focus behavior.
                event.preventDefault();
                // or return false;
            }
        });
    },
    _handleClickEvent: function () {
        var self = this;
        // Handles when a visitor clicks a query completion
        $(document).on('click', '.autocomplete-result > ul > li', function () { // we need to use "on" function since the suggestions are created after the dom is loaded
            self._doSearch(this);
            return false;
        });

        $(document).on('keyup', '.autocomplete-result li', function (e) {
            var keyCode = e.keyCode || e.which;

            if (keyCode == 13) { // Enter
                e.preventDefault();
                self._doSearch(e.target);
            }
        });
    },
    _addSearchQueries: function (queries, container, searchField) {

        //IS_IPAD = navigator.userAgent.match(/iPad/i) != null;
        //IS_IPHONE = navigator.userAgent.match(/iPhone/i) != null || (navigator.userAgent.match(/iPod/i) != null);

        if (queries.length < 1) return;

        var ul = container.append('<ul></ul>').find('ul');
        var template = $('div[data-id="template"]');
        var self = this;

        $.each(queries, function (i) {

            var label_hl = '', vgrid = '';

            if (queries[i].label_hl)
                label_hl = queries[i].label_hl;

            if (queries[i].vgrid)
                vgrid = queries[i].vgrid;

            var vgridElement = $(template).find('.vgr-quick-search__id').clone(); // Clone the element because it exists inside the headline.
            $(template).find('.vgr-quick-search__headline').html($.parseHTML(label_hl));
            $(template).find('.vgr-quick-search__headline').append(vgridElement.html($.parseHTML(vgrid))); // Append the clone

            //var bestalladId = self._getBestallarIds(queries[i].bestallar_id_benamning);

            self._addSuggestionNumbersOrHide(template, queries[i].bestallar_id, '.bNumber');
            self._addSuggestionNumbersOrHide(template, queries[i].kiv_ansvarsnummer, '.aNumber');
            self._addSuggestionNumbersOrHide(template, queries[i].i_number, '.iNumber');
        
            ul.append('<li tabindex="0" aria-selected="false" role="link" data-query="' + queries[i].query + '">' + template.html() + '</li>');

        //var li = $("<li/>").appendTo(ulList);
        //var h3 = $("<h3>").appendTo(li);
        //var aaa = $("<a>", {
        //    html: queries[i].label_hl,
        //    href: url
        //}).appendTo(h3);
        //if (IS_IPAD || IS_IPHONE) {
        //    aaa.on('click touchend', function () {
        //        var link = $(this).attr('href');
        //        window.open(link, '_self');
        //        return false;
        //    });
        //}
        });

        // If there's 6 or more items we'll also add a link that executes a normal search.
        // Some user thougt that the query completion contained all search hits and this should solve that problem.
        if (queries.length >= 6) {
            ul.append('<li tabindex="0" aria-selected="false" role="link" data-query="q=' + searchField.val() + '" class="vgr-quick-search__inline-list-show-all">Klicka här för att visa alla sökresutat</li>');
        }
    },
// NOTE: We currently don't take cirkulationsordning into consideration
    // bestallar_id_benamning is a string split into three fields, separeated by a "$" sign. 
    //_getBestallarIds: function (unparsedBestallarIdBenamning) {
    //    var commaSeparatedBestallarIds = '';
        
    //    if (unparsedBestallarIdBenamning) {
    //        // Multiple id's are separeated by a comma
    //        var allBestallarIdBenamning = unparsedBestallarIdBenamning.split(',');

    //        $.each(allBestallarIdBenamning, function (i, value) {
    //            var split = value.split('$');

    //            if (split.length == 3) {
    //                // The last fields tells us what this id is for. All bestallar-id's ends with a 1. (Other fields serves a different purpose and we dont want them in the query completion)
    //                if (split[2] == '1') {
    //                    // The first field contains the id.
    //                    commaSeparatedBestallarIds += split[0] + ',';
    //                }
    //            }
    //        });
    //    }
    //    // Remove trailing comma
    //    commaSeparatedBestallarIds = commaSeparatedBestallarIds.replace(/,\s*$/, "");
    //    return commaSeparatedBestallarIds;
    //},
_addSuggestionNumbersOrHide: function (template, suggestionData, appendToSelector) {

    if (suggestionData && suggestionData != '') {

        // If there's more than one item
        if (suggestionData.split(',').length > 1) {
            // Then show a text insted that tells the user to click this suggestion to show all items
            var clickToShowAllHtml = '<span class="bestallar-id__show-more">' +
            'Flera alternativ tillgängliga' +
            '</span>' +
            '<svg class="vgr-quick-search__inline-list-icon bestallar-id__expand-close-icon" fill="#A9A9A9" height="15" viewBox="0 0 24 24" width="15" xmlns="http://www.w3.org/2000/svg">' +
            '<path d="M0 0h24v24H0z" fill="none" />' +
            '<path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm5 11h-4v4h-2v-4H7v-2h4V7h2v4h4v2z" />' +
            '</svg>';
            suggestionData = clickToShowAllHtml;
        }

        // Show suggestions
        $(template).find(appendToSelector).html(suggestionData);
        $(template).find(appendToSelector).closest('.vgr-quick-search__inline-list-item').removeClass('hidden');
    }
    else
        // Hide suggestions
        $(template).find(appendToSelector).closest('.vgr-quick-search__inline-list-item').addClass('hidden');
},
_addCloseResultContainerListener: function (searchField, searchButton, resultContainer) {
    this._closeResultContainerMousedown(searchField, searchButton, resultContainer);
    this._closeResultContainerFocusin(searchField, searchButton, resultContainer);
},
_closeResultContainerMousedown: function (searchField, searchButton, resultContainer) {
    var self = this;
    $("body").click(function (e) {

        if (self._shouldHideSuggestions(e.target, searchField, resultContainer))
            resultContainer.addClass("hidden");
    });
},
_closeResultContainerFocusin: function (searchField, searchButton, resultContainer) {
    var self = this;
    $("body").keyup(function (e) {
        var keyCode = e.keyCode || e.which;

        if (keyCode == 9) { // Tab

            if (self._shouldHideSuggestions(e.target, searchField, resultContainer)) {
                resultContainer.addClass("hidden");
            }
        }
        else if (keyCode == 27) { // Esc
            resultContainer.addClass("hidden");
        }
    });
},
_shouldHideSuggestions: function (target, searchField, resultContainer) {
    // Look upward to find the query completion container
    var closestResultContainer = $(target).closest('#' + resultContainer.attr('id'));
    var suggestionIsFocused = closestResultContainer && closestResultContainer.length > 0;

    // If the search input field is selected we still want to show the query completion
    if (!resultContainer.hasClass("hidden") && !suggestionIsFocused && target.id != searchField.attr('id')) {
        return true;
    }
    return false;
},
_doSearch: function (selectedSuggestion) {

    if (!selectedSuggestion)
        return;

    var resultPageUrl = $(selectedSuggestion).closest('form').data('result-page-url');
    var query = $(selectedSuggestion).data('query'); // Get the clicked suggestion
    window.location.href = resultPageUrl + "?" + query; // Show search result page
},
});
///#source 1 1 /Static/js/widgets/expanded-block-toggle-event.js

// If the page contains any expandable blocks or vgr tabs
if ($('[data-expandblock]').length > 0) {

    // "selectionchange" works differentlt on Chrome and we cannot search/open content of the block.
    if (!Vgr.Util.isChrome()) {

        // Add an event listener that is called whenever an element is clicked or selected etc.
        // We do this because when a user searches the web page (ctrl+f) and finds a result inside an expandable block, the content should be visible.
        document.addEventListener("selectionchange", function (e) {
            var selection = document.getSelection();

            var anchorNode = selection.anchorNode;

            if (anchorNode === null)
                return;

            var $expandableContent = $(selection.anchorNode.parentNode).closest('[data-expandblockcontent]');

            // Check that the selected content is inside an expandable block
            if ($expandableContent !== null && $expandableContent.length > 0) {

                var $expandableBlock = $expandableContent.closest('[data-expandblock]');

                // Open the expandable content
                if (!$expandableBlock.hasClass('expand-block--open'))
                    $expandableBlock.addClass('expand-block--open');
                $expandableContent.removeClass('expand-block--not-show');

                $expandableContent.attr('aria-expanded', true);
            }
        });
    }
    else {

    }
}

$('.vgr-toggle__button').click(function () {

    var toggleTexts = $(this).children('.toggle-text').children('span');

    $(toggleTexts).each(function () {
        if ($(this).hasClass("hidden")) {
            $(this).removeClass("hidden");
        } else {
            $(this).addClass("hidden");
        }
    });

    return false; // If the click event is executed the search form will be submitted
});
///#source 1 1 /Static/js/widgets/vgr-tabs.js
$(document).ready(function () {

    // Ta bort alla display:none och ersätter det med vgr-visually-hidden.
    // Detta är för att JQuery UI sätter display:none på alla flikar när sidan laddas som inte fungera med sökningen i webbläsaren.

    var allPanel = $(document.querySelectorAll('.ui-tabs-panel'));
    for (i = 0; i <= allPanel.length; i++) {
        if (allPanel[i] !== undefined) {
            if (allPanel[i].hasAttribute('style') === "display=none") {
                $(allPanel[i]).removeAttr('style');
                $(allPanel[i]).addClass('vgr-visually-hidden');
            }

            else {
                $(allPanel[i]).removeClass('vgr-visually-hidden');

            }
            if ($(allPanel[i]).css("display") === "none") {
                $(allPanel[i]).css("display") === "";
                $(allPanel[i]).addClass('vgr-visually-hidden');
            }
            continue;
        }
    }
    $(document).keyup(function (ev) {
        if (ev.which == 37 || ev.which == 39) {
            $(ev.target).children('a').click()
        }
    });
    // ta bort vgr-visually-hidden vid en click 
    $('.ui-tabs-anchor').on('click', function (event) {
        var panel = document.querySelectorAll('.ui-tabs-panel');
        for (i = 0; i <= panel.length; i++) {
            if (panel[i] !== undefined) {
                // JQuery UI lägger på display:none när man växlar tab , behöver ta bort den igen
                if (panel[i].hasAttribute('style') === "display=none") {
                    $(panel[i]).removeAttr('style');
                    $(panel[i]).addClass('vgr-visually-hidden');
                }

                if ($(panel[i]).css("display") === "none") {
                    $(panel[i]).css('display', 'block');
                    $(panel[i]).addClass('vgr-visually-hidden');
                }


                if (($(panel[i]) !== undefined) && ($(this).attr('href') === '#' + $(panel[i]).attr('id') + '')) {
                    if ($(panel[i]).hasClass('vgr-visually-hidden')) {
                        $(panel[i]).removeClass('vgr-visually-hidden');
                    }
                }

                continue;
            }
        }     
    });

    if ($('[data-vgr-tabs]').length > 0) {

        // "selectionchange" works differentlt on Chrome and we cannot search/open content of the area.
        if (!Vgr.Util.isChrome()) {

            // Add an event listener that is called whenever an element is clicked or selected etc.
            // We do this because when a user searches the web page (ctrl+f) and finds a result inside an tabs area, the content should be visible.

            document.addEventListener("selectionchange", function (e) {
                var selection = document.getSelection();
                if ($(selection.anchorNode) !== undefined && $(selection.anchorNode) !== null) {
                    var $dataContent = $(selection.anchorNode.parentNode).closest('[data-tabcontent]');

                    // Check that the selected content is inside an tab content
                    if ($dataContent !== null && $dataContent.length > 0) {
                        var id = $dataContent.attr("id");

                        var tab = $(document).find('.ui-tabs-anchor');
                        for (var i = 0; i < tab.length; i++) {
                            if ($(tab[i]).attr('href') === '#' + id + '') {
                                $dataContent.removeClass('vgr-visually-hidden');
                                $(tab[i]).click();

                            }

                            continue;
                        }

                    }
                }
            });
        }
    }
});

///#source 1 1 /Static/js/widgets/get-username.js
$.widget('customer.getTopBarInfo', {
    options: {
    },

    // Constructor
    _create: function () {
        this._getuser();
    },
    _destroy: function () {
    },
    _getuser: function () {
        var self = this;
        var body = $("body");
        var pageid = self._getMeta('SidID');
        $.ajax({
            url: '/accountinfo/getsession', // NOTE: The path must be lower case because the request is cached in lower case by Varnish
            type: 'GET',
            data: $.param({ pageid: pageid }),
            dataType: 'json',
            success: function (data) {

                if (data.Auth == true) {

                    if (body.data("pagetype") === "StartPage") {
                        var $welcomeHeading = $('#h1id');
                        if ($welcomeHeading.length > 0) {
                            // Append the first name after the welcome text
                            $('#h1id').text(data.lang + ' ' + data.Firstname);
                        }
                    }

                    var $ContactForm = $('#page-footer-contact-form');

                    if ($ContactForm.length > 0) {

                        var name = $('#contact-form-name');
                        if (name) {
                            name[0].value = data.Name;

                        }
                        var phone = $('#contact-form-phone');
                        if (phone) {
                            phone[0].value = data.telephonenr;
                        }

                        var email = $('#contact-form-email');
                        if (email) {
                            email[0].value = data.mail;
                        }


                    }

                    // Show the element by removeing 'style="display:none"'
                    self.element[0].getElementsByTagName('div')[0].removeAttribute("style");
                    if (data.Link !== "") {

                        self.element[0].getElementsByClassName('personlink')[0].href = data.Link;
                    }

                    if (data.Name !== "") {

                        self.element[0].getElementsByClassName('personlink')[0].getElementsByTagName('span')[0].innerText = data.Name;
                    }

                    if (data.Photo !== "") {
                        self.element[0].getElementsByClassName('personlink')[0].getElementsByTagName('img')[0].src = data.Photo;
                    }



                }



                //if (data.Photo !=="") {
                //    self.element[0].getElementsByTagName('img')[0].attribute('src') = data.Photo;

                //}

            },
            async: true,
            processData: false
        });

    },

    _getMeta: function (metaName) {
        const metas = document.getElementsByTagName('meta');

        for (let i = 0; i < metas.length; i++) {
            if (metas[i].getAttribute('name') === metaName) {
                return metas[i].getAttribute('content');
            }
        }

        return '';
    }

});


///#source 1 1 /Static/js/ifeed-block.js
$.widget('customer.ifeedblock', {
    options: {

    },

    // Constructor
    _create: function () {
        this.setupClickEvents();
        this.setSortOrderTitelAttr();
    },
    _destroy: function () {
    },
    setupClickEvents: function () {

        // Alla sorterings alternativ som finns
        var sortLinks = $('th a', this.element);

        var self = this;
        $.each(sortLinks, function () {
            // När man klickar på en sorterings knapp
            $(this).click(function () {

                // Visa laddnings-snurran
                $(self.element).find('.ajax-loader-img').removeClass('hidden');
            });
        });
    },
    setSortOrderTitelAttr: function () {

        // Alla sorterings alternativ som finns
        var sortLinks = $('th a', this.element);

        $.each(sortLinks, function () {

            // Hämta länk texten och ta bort sorterings ikonerna för de behöver inte vara med i titel attributet på länken
            var linkText = $(this).text().replace('▲', '').replace('▼', '');

            // Hämta "sortdir" parametern från länken. Den avgör vad som kommer att appliceras när man klickar på knappen
            var selectableSortDirection = Vgr.Util.getParameterByName('sortdir', this.href);

            var titleText = '';

            if (selectableSortDirection == 'DESC') {
                titleText = 'Sortera på ' + linkText + ' i fallande ordning';
            }
            else if (selectableSortDirection == 'ASC') {
                titleText = 'Sortera på ' + linkText + ' i stigande ordning';
            }

            // Sätt titel attributet för länkarna
            $(this).prop('title', titleText)
        });
    }
});

///#source 1 1 /Static/js/custom-Epi-quickNavigator.js
$(function () {

    var customQuickNavElem = $('#custom-QuickNavigator');
    if (customQuickNavElem.length === 1) {
        
        $.ajax({
            url: '/CustomQuickNavigator/GetNavigatorSettings',
            type: 'GET',
            dataType: 'json',
            data: {
                currentUrl: window.location.href,
                referrer: document.referrer
            },
            success: function (result) {
                if (result) {
                    if (result.shownav === true) {
                       
                        // Kolla att Quick Navigatorn som Epi skapar inte finns
                        if ($('#epi-quickNavigator').length === 0) {

                            // Läs in css för quickNavigator
                            var host = window.location.protocol + '//' + window.location.hostname;
                            $('head').append('<link rel="stylesheet" href="' + host + '/util/styles/quicknavigator.css" type="text/css' + '" />');

                            var currentPageId = customQuickNavElem.data('currentpageid');

                            var html = '<ul id="epi-quickNavigator">' +
                                '<li class="epi-quickNavigator-editLink">' +
                                '<a href="https://' + result.hostAndPath + '?language=sv#context=epi.cms.contentdata:///' + currentPageId + '" target="_self"><span>Episerver</span></a>' +
                                '</li>' +
                                '<li class="epi-quickNavigator-dropdown">' +
                                '<a id="epi-quickNavigator-clickHandler" class="epi-quickNavigator-dropdown-arrow" href="javascript:void(0)"></a>' +
                                '<ul id="epi-quickNavigator-menu" style="display: none;">' +
                                '<li>' +
                                '<a href="https://' + result.hostAndPath.slice(0, result.hostAndPath.lastIndexOf('/')) + '" target="_self">Dashboard</a>' +
                                '</li>' +
                                '<li>' +
                                '<a href="https://' + result.hostAndPath + '?language=sv#context=epi.cms.contentdata:///' + currentPageId + '" target="_self">CMS-redigering</a>' +
                                '</li></ul></li></ul>';
                            $('body').append(html);

                            //Toggle div when button is clicked 
                            $('#epi-quickNavigator-clickHandler').on("click", function (e) {
                                $("#epi-quickNavigator-menu").toggle();
                                e.preventDefault();
                            });
                        }
                    }
                }
            },
            error: function (error) {
                console.log('Anropet misslyckades. Error', error);
            }

        });
    }
});
