    key: "expandMenu",
    value: function expandMenu(menuItem) {
      var focusMode = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 'first';
      this.menuExpanded = true;

      if (menuItem.getAttribute('aria-haspopup') !== 'true') {
        return;
