    //        var $clicked = $(e.target);
    //        if (!$clicked.parents().hasClass("job-search")) $(".job-search #checkboxes ul").hide();
    //    });

    //    $('#checkboxes ul li input[type="radio"]').on('click', function () {

    //        var title = $(this).closest('#checkboxes').find('input[type="radio"]').val(),
    //            title = $(this).val() + ",";

